<!DOCTYPE html>
<html lang="en">
<%- include('layout/head', { title: title }) %>
<body>
<%- include('layout/header') %>
<main>
    <div class="content-wrapper">
        <h1>All photos</h1>
        <ul class="photos">
            <% for (let i = 0; i < photos.length; i++) { %>
                <li>
                    <%- include('photos/photo', { photo: photos[i] }) %>
                </li>
            <% } %>
        </ul>

        <%- include('photos/pagination', { pagination: pagination }) %>
    </div>
</main>
<%- include('layout/footer') %>
</body>
</html>
